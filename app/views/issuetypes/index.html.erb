<h1>Listing issuetypes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @issuetypes.each do |issuetype| %>
  <tr>
    <td><%= issuetype.name %></td>
    <td><%= issuetype.active %></td>
    <td><%= link_to 'Show', schema_issuetype_path %></td>
    <td><%= link_to 'Edit', edit_schema_issuetype_path(issuetype) %></td>
